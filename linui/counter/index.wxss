.container-count {
    flex-direction: row;
    height: 56rpx;
    width: 170rpx;
}

.container-count,.symbol {
    display: flex;
}

.symbol {
    align-items: center;
    color: #333;
    font-size: 28rpx;
    height: 100%;
    justify-content: center;
    width: 56rpx;
}

.disabled {
    color: #949494;
}

.abled,.disabled {
    background-color: #fff;
}

.abled,.count {
    color: #333;
}

.count {
    background: #f5f5f5;
    flex: 1;
    font-size: 24rpx;
    height: 100%;
    line-height: 56rpx;
    min-height: 56rpx;
    overflow: hidden;
    text-align: center;
    text-overflow: ellipsis;
    white-space: nowrap;
}

@font-face {
    font-family:iconfont;src:url("data:application/x-font-woff2;charset=utf-8;base64,") format("woff2");
}

.l-icon-add:before {
    content: "\e602";
}

.l-icon-reduce:before {
    content: "\e69c";
}

.l-icon {
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    font-family: iconfont;
    font-style: normal;
}

.l-icon::before {
    display: inline-flex;
}

.count-hover {
    opacity: .8;
}