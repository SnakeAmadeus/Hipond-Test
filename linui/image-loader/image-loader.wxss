@-webkit-keyframes fadeIn {
    0% {
        opacity: 0;
    }

    50% {
        opacity: .5;
    }

    100% {
        opacity: 1;
    }
}

@keyframes fadeIn {
    0% {
        opacity: 0;
    }

    50% {
        opacity: .5;
    }

    100% {
        opacity: 1;
    }
}

.fade_in {
    animation: fadeIn 1s ease;
    background: #f5f5f5;
    border-radius: 8rpx;
    height: 100%;
    width: 100%;
}